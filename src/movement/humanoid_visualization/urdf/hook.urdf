<?xml version="1.0"?>

<robot name="nova">

  <material name="black">
    <color rgba="0 0 0 1"/>
  </material>
  <material name="white">
    <color rgba="1 1 1 1"/>
  </material>
  <material name="red">
    <color rgba="1 0 0 0.2"/>
  </material>
  <material name="blue">
    <color rgba="0 0 1 0.2"/>
  </material>
  <material name="green">
    <color rgba="0 1 0 0.2"/>
  </material>

  <link name="base_link">
    <visual>
      <geometry>
        <sphere radius="0.005"/>
      </geometry>
      <material name="white"/>
    </visual>
  </link>

  <link name="x_axis">
    <visual>
      <geometry>
				<cylinder radius="0.005" length="5"/>
			</geometry>
      <origin rpy="0 1.57075 0" xyz="2.5 0 0"/>
      <material name="red"/>
    </visual>
  </link>

  <joint name="base_to_x_axis" type="fixed">
    <parent link="base_link"/>
    <child link="x_axis"/>
    <origin xyz="0 0 0"/>
  </joint>

  <link name="y_axis">
    <visual>
      <geometry>
				<cylinder radius="0.005" length="5"/>
			</geometry>
      <origin rpy="1.57075 0 0" xyz="0 2.5 0"/>
      <material name="green"/>
    </visual>
  </link>

  <joint name="base_to_y_axis" type="fixed">
    <parent link="base_link"/>
    <child link="y_axis"/>
    <origin xyz="0 0 0"/>
  </joint>

  <link name="z_axis">
    <visual>
      <geometry>
				<cylinder radius="0.005" length="5"/>
			</geometry>
      <origin xyz="0 0 2.5"/>
      <material name="blue"/>
    </visual>
  </link>

  <joint name="base_to_z_axis" type="fixed">
    <parent link="base_link"/>
    <child link="z_axis"/>
    <origin xyz="0 0 0"/>
  </joint>

  <link name="COM">
    <visual>
      <geometry>
				<cylinder radius="0.085" length="0.06"/>
			</geometry>
      <origin xyz="0 0 0.03"/>
      <material name="black"/>
    </visual>
  </link>

  <joint name="base_to_COM" type="fixed">
    <parent link="base_link"/>
    <child link="COM"/>
    <origin xyz="0 0 0"/>
  </joint>

  <link name="BASE_UZ">
    <visual>
			<geometry>
				<cylinder radius="0.01" length="0.03"/>
			</geometry>
      <origin xyz="0 0 0"/>
      <material name="black"/>
    </visual>
  </link>

  <joint name="BASE_UZ_joint" type="revolute">
    <axis xyz="0 0 1"/>
		<limit effort="1000.0" lower="-3.14" upper="3.14" velocity="10"/>
    <origin rpy="0 0 0" xyz="0.0 0.0 0.06124"/>
    <parent link="COM"/>
    <child link="BASE_UZ"/>
  </joint>

  <link name="SHOULDER_UY">
    <visual>
			<geometry>
				<cylinder radius="0.01" length="0.03"/>
			</geometry>
      <origin rpy="1.57075 0 0" xyz="0 0 0"/>
      <material name="black"/>
    </visual>
  </link>

  <joint name="SHOULDER_UY_joint" type="revolute">
    <axis xyz="0 1 0"/>
		<limit effort="1000.0" lower="-3.14" upper="3.14" velocity="10"/>
    <origin rpy="0 0 0" xyz="0 0 0.05410"/>
    <parent link="BASE_UZ"/>
    <child link="SHOULDER_UY"/>
  </joint>

  <link name="ELBOW_UY">
    <visual>
      <geometry>
				<cylinder radius="0.01" length="0.03"/>
			</geometry>
      <origin rpy="1.57075 0 0" xyz="0 0 0"/>
      <material name="black"/>
    </visual>
  </link>

  <joint name="ELBOW_UY_joint" type="revolute">
    <axis xyz="0 1 0"/>
		<limit effort="1000.0" lower="-3.14" upper="3.14" velocity="10"/>
    <origin rpy="0 0 0" xyz="0 0 0.16000"/>
    <parent link="SHOULDER_UY"/>
    <child link="ELBOW_UY"/>
  </joint>

	<link name="WRIST_UY">
    <visual>
      <geometry>
				<cylinder radius="0.01" length="0.03"/>
			</geometry>
      <origin rpy="1.57075 0 0" xyz="0 0 0"/>
      <material name="black"/>
    </visual>
  </link>

  <joint name="WRIST_UY_joint" type="revolute">
    <axis xyz="0 1 0"/>
		<limit effort="1000.0" lower="-3.14" upper="3.14" velocity="10"/>
    <origin rpy="0 0 0" xyz="0 0 0.09400"/>
    <parent link="ELBOW_UY"/>
    <child link="WRIST_UY"/>
  </joint>

	<link name="WRIST_UZ">
    <visual>
      <geometry>
				<cylinder radius="0.01" length="0.03"/>
			</geometry>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <material name="black"/>
    </visual>
  </link>

  <joint name="WRIST_UZ_joint" type="revolute">
    <axis xyz="0 0 1"/>
		<limit effort="1000.0" lower="-3.14" upper="3.14" velocity="10"/>
    <origin rpy="0 0 0" xyz="0 0 0.02435"/>
    <parent link="WRIST_UY"/>
    <child link="WRIST_UZ"/>
  </joint>

	<link name="GRIPPER">
    <visual>
      <geometry>
				<box size="0.097 0.049 0.0678"/>
			</geometry>
      <origin rpy="0 0 0" xyz="0 0 0.0339"/>
      <material name="black"/>
    </visual>
  </link>

  <joint name="GRIPPER_UX_joint" type="fixed">
    <origin rpy="0 0 0" xyz="0 0 0"/>
    <parent link="WRIST_UZ"/>
    <child link="GRIPPER"/>
  </joint>

</robot>